                        x1 = axisx.min;
                    }
                    else if (x2 &lt;= x1 &amp;&amp; x2 &lt; axisx.min) {
                        if (x1 &lt; axisx.min)
                            continue;
                        y2 = (axisx.min - x1) / (x2 - x1) * (y2 - y1) + y1;
                        x2 = axisx.min;
                    }

                    // clip with xmax
                    if (x1 &gt;= x2 &amp;&amp; x1 &gt; axisx.max) {
                        if (x2 &gt; axisx.max)
                            continue;
                        y1 = (axisx.max - x1) / (x2 - x1) * (y2 - y1) + y1;
                        x1 = axisx.max;
                    }
                    else if (x2 &gt;= x1 &amp;&amp; x2 &gt; axisx.max) {
                        if (x1 &gt; axisx.max)
                            continue;
